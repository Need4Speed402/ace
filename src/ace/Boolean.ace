.cmp : unsafe identifier compare

value, arg, (
	;; run cb if true
	cmp *arg ? , cb, (
		value cb () ()
	)
	
	;; run cb if false
	cmp *arg !? , cb, (
		value () cb ()
	)
	
	;; invert
	cmp *arg `!` {
		value false true
	}
	
	;; xor
	cmp *arg ! , v2, (
		value {!v2} {v2} ()
	)
	
	;; and
	cmp *arg & , v2, (
		value v2 false
	)
	
	;; or
	cmp *arg | , v2, (
		value true v2
	)
	
	;; not equal
	cmp *arg ~= , v2, (
		value {!v2} {v2} ()
	)
	
	;; equal
	cmp *arg = , v2, (
		value {v2} {!v2} ()
	)
)