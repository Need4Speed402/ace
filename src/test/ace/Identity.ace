test {
	name 'basic identity'
	case {
		{
			[console] hello
		} [root Procedure]
	}
	expect "
		hello
		
	`"
}

test {
	name 'basic identity return ident'
	case {
		[console] (([root Function] param {
			param
		}) stuff)
	}
	expect "
		stuff
		
	`"
}

test {
	name 'basic identity nested preserve identifier'
	case {
		[console] ({
			{
				{
					{
						hello
					} [root Procedure]
				} [root Procedure]
			} [root Procedure]
		} [root Procedure])
	}
	expect "
		hello
		
	`"
}

test {
	name 'basic identity nested'
	case {
		{
			{
				{
					{
						[console] hello
					} [root Procedure]
				} [root Procedure]
			} [root Procedure]
		} [root Procedure]
	}
	expect "
		hello
		
	`"
}

test {
	name 'basic identity nested with do'
	case {
		{
			[root ``] ([console] hello) ({
				{
					{
						[console] world
					} [root Procedure]
				} [root Procedure]
			} [root Procedure])
		} [root Procedure]
	}
	expect "
		hello
		world
		
	`"
}