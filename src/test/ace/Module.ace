test {
	name 'basic package'
	package {
		name root
		case main {
			[console] hello
		}
	}
	entry main
	expect "
		hello
		
	`"
}

test {
	name 'basic root package'
	package {
		name root
		unsafe
		package {
			name import
			case main {
				[[
					unsafe console hello
				]]
			}
			case root {
				unsafe root
			}
		}
	}
	entry (import main)
	expect "
		hello
		
	`"
}