test {
	name 'basic test succcess'
	case {
		[console] (
			[compare] a a true false
		)
	}
	expect "
		true
		
	`"
}

test {
	name 'basic test fail'
	case {
		[console] (
			[compare] a b true false
		)
	}
	expect "
		false
		
	`"
}

test {
	name 'effects in single identifier'
	case {
		[compare] ([console] hello)
	}
	expect "
		hello
		
	`"
}

test {
	name 'effects in compared identifiers'
	case {
		[compare] ([console] hello) ([console] world)
	}
	expect "
		hello
		world
		
	`"
}

test {
	name 'effects in compared identifiers success'
	case {
		[console] (
			[compare] ([console] hello) ([console] hello) true false
		)
	}
	expect "
		hello
		hello
		true
		
	`"
}

test {
	name 'effects in compared identifiers failure'
	case {
		[console] (
			[compare] ([console] hello) ([console] world) true false
		)
	}
	expect "
		hello
		world
		false
		
	`"
}

test {
	name 'compare first unknown success'
	case {
		[console] (([root Function] a {
			[compare] a hello true false
		}) hello)
	}
	expect "
		true
		
	`"
}

test {
	name 'compare first unknown failure'
	case {
		[console] (([root Function] a {
			[compare] a world true false
		}) hello)
	}
	expect "
		false
		
	`"
}

test {
	name 'compare second unknown success'
	case {
		[console] (([root Function] b {
			[compare] hello b true false
		}) hello)
	}
	expect "
		true
		
	`"
}

test {
	name 'compare second unknown failure'
	case {
		[console] (([root Function] b {
			[compare] hello b true false
		}) world)
	}
	expect "
		false
		
	`"
}

test {
	name 'compare unknown values success compare indirection'
	case {
		([root Function] compare {
			[console] (([root Function] a {
				([root Function] b {
					compare a b true false
				})
			}) hello hello)
		}) [compare]
	}
	expect "
		true
		
	`"
}

test {
	name 'compare unknown values failure compare indirection'
	case {
		([root Function] compare {
			[console] (([root Function] a {
				([root Function] b {
					compare a b true false
				})
			}) hello world)
		}) [compare]
	}
	expect "
		false
		
	`"
}

test {
	name 'compare common identifier'
	case {
		[console] ([root Function] param {
			[compare] param param true false
		} hello)
	}
	expect "
		true
		
	`"
}
